{
  "family": "boom-app-family",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "lendsqr_backend",
      "image": "",
      "memory": 512,
      "cpu": 128,
      "environment": [
        {
          "name": "DB_USER",
          "value": "placeholder"
        },
        {
          "name": "PASSWORD",
          "value": "placeholder"
        },
        {
          "name": "CLUSTERNAME",
          "value": "placeholder"
        }
      ],
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8000
        }
      ]
    },
    {
      "name": "lendsqr",
      "image": "",
      "memory": 512,
      "cpu": 128,
      "environment": [
        {
          "name": "REACT_APP_LENDSQR_API_URL",
          "value": "placeholder"
        },
        {
          "name": "REACT_APP_MEDIA_URL",
          "value": "placeholder"
        }
      ],
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000
        }
      ]
    }
  ],
  "cpu": "0.25 vCPU",
  "memory": "0.5 GB",
  "executionRoleArn": "arn:aws:iam::378523290404:role/ECSExecutionRole",
  "runtimePlatform": { "operatingSystemFamily": "WINDOWS_SERVER_2019_CORE" },
  "requiresCompatibilities": ["FARGATE"]
}
